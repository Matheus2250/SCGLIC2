# Configuração para Produção/EXE
# Substitua os valores abaixo pela configuração do seu servidor

# Para Banco Local (PostgreSQL deve estar instalado na máquina)
DATABASE_URL=postgresql://postgres:123@localhost:5432/sistema_contratacoes?client_encoding=utf8

# Para Banco Remoto (substitua SEU_IP pelo IP do seu servidor)
# DATABASE_URL=postgresql://postgres:123@SEU_IP:5432/sistema_contratacoes?client_encoding=utf8

# Configurações de Segurança
SECRET_KEY=sua-chave-secreta-super-segura-aqui-mude-em-producao
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
ENVIRONMENT=production

# Configurações de Encoding
PYTHONIOENCODING=utf-8
PYTHONUTF8=1